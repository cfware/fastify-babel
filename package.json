{
	"name": "fastify-babel",
	"version": "0.1.0",
	"description": "Fastify Babel plugin",
	"main": "index.js",
	"scripts": {
		"test": "xo && nyc ava"
	},
	"engines": {
		"nodejs": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"*.js"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/fastify-babel.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/fastify-babel/issues"
	},
	"homepage": "https://github.com/cfware/fastify-babel#readme",
	"dependencies": {
		"@babel/core": "^7.0.0-beta.47",
		"fastify-plugin": "^1.0.1"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"babel-plugin-bare-import-rewrite": "^0.1.1",
		"fastify": "^1.5.0",
		"fastify-static": "^0.12.0",
		"got": "^8.3.1",
		"nyc": "^11.8.0",
		"string-to-stream": "^1.1.1",
		"xo": "^0.21.1"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"*.js",
			"!test.js"
		]
	},
	"xo": {
		"rules": {
			"comma-dangle": [
				"error",
				"always-multiline"
			]
		}
	}
}
